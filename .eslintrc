{
  "extends": "airbnb",
  "globals": {
    "log": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "semi": [2, "never"],
    "global-require": 0,
    "indent": [2, 4],
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0
  }
}
